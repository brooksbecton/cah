{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "globals": {
    "document": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["immutable"],
  "rules": {
    "immutable/no-let": 2,
    "immutable/no-mutation": 2
  }
}
