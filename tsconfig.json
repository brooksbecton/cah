{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "jsx": "react",
      "lib": ["es6"],
      "module": "es6",
      "moduleResolution": "node",
      "noEmit": true,
      "noImplicitAny": true,
      "target": "es6",
      "baseUrl": "./",
      "paths": {
        "@assets/*": ["assets/*"],
        "@config/*": ["config/*"],
        "@shared/*": ["src/shared/*"]
      }
    },
    "exclude": ["node_modules"]
  }
  